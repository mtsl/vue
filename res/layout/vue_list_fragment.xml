<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/vue_list_fragment_sidemenu_actionbar"
        android:layout_width="fill_parent"
        android:layout_height="48dp"
        android:layout_alignParentTop="true"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/list_cancle"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="2dp"
            android:background="@drawable/action_search" />

        <EditText
            android:id="@+id/side_Menu_searchBar"
            android:layout_width="fill_parent"
            android:layout_height="42dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:layout_toRightOf="@+id/list_cancle"
            android:focusableInTouchMode="true"
            android:gravity="center_vertical"
            android:hint="@string/hint_search"
            android:imeOptions="actionSearch"
            android:singleLine="true"
            android:textColor="#ff0000" />
    </RelativeLayout>

    <ExpandableListView
        android:id="@+id/vue_list_fragment_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/vue_list_fragment_sidemenu_actionbar"
        android:divider="@null"
        android:fadingEdge="none"
        android:groupIndicator="@null"
        android:scrollbars="none" />

    <LinearLayout
        android:id="@+id/customlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone" >

        <!-- android:background="@drawable/appbg" -->

        <TextView
            android:id="@+id/setsizeid"
            android:layout_width="fill_parent"
            android:layout_height="42dp"
            android:layout_marginTop="0dp"
            android:background="@color/red"
            android:gravity="left|center_vertical"
            android:paddingLeft="12dp"
            android:text="@string/profile"
            android:textColor="#ffffff" />
      <RelativeLayout 
            android:id="@+id/user_info"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            >
            <ImageView
                android:id="@+id/user_profilePic"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:contentDescription="@string/image_contentDescription"
                />
          <TextView
              android:id="@+id/user_name"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Name:"
              android:layout_below="@+id/user_profilePic"              
              />
          <TextView
              android:id="@+id/user_date_Of_birth"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="DOB:"
              android:layout_below="@+id/user_name"
              />
          <TextView
              android:id="@+id/user_gender"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Gender:"
              android:layout_below="@+id/user_date_Of_birth"
              />
          <TextView
              android:id="@+id/user_current_location"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Current Location:"
              android:layout_below="@+id/user_gender"
              />
      </RelativeLayout>
